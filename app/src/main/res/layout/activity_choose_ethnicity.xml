<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/american_flag"
    tools:context=".ChooseEthnicity">

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="35dp"
        android:layout_height="49dp"
        app:layout_constraintBottom_toBottomOf="@+id/blackMaleImageButtonID"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/blackMaleImageButtonID"
        app:layout_constraintTop_toTopOf="@+id/blackMaleImageButtonID"
        app:layout_constraintVertical_bias="0.818"
        app:srcCompat="@drawable/greendollarsymbol" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="39dp"
        android:layout_height="49dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="12dp"
        app:layout_constraintEnd_toEndOf="@+id/imageView6"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/imageView6"
        app:layout_constraintTop_toBottomOf="@+id/imageView6"
        app:srcCompat="@drawable/greendollarsymbol" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="39dp"
        android:layout_height="54dp"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/tanMaleImageButtonID"
        app:layout_constraintEnd_toStartOf="@+id/tannerMaleImageButtonID"
        app:layout_constraintTop_toBottomOf="@+id/americaTheeGuideTextViewID2"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/greendollarsymbol" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="35dp"
        android:layout_height="49dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginBottom="32dp"
        app:layout_constraintBottom_toTopOf="@+id/whiteFemaleImageButtonID"
        app:layout_constraintEnd_toStartOf="@+id/imageView3"
        app:layout_constraintHorizontal_bias="0.692"
        app:layout_constraintStart_toStartOf="@+id/imageView"
        app:srcCompat="@drawable/greendollarsymbol" />

    <TextView
        android:id="@+id/americaTheeGuideTextViewID2"
        android:layout_width="660dp"
        android:layout_height="68dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:text="@string/americaChooseWhoYouAre"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:textColor="@android:color/darker_gray"
        android:textSize="52sp"
        app:fontFamily="@font/lobster"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.095" />

    <ImageButton
        android:id="@+id/whiteMaleImageButtonID"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintEnd_toStartOf="@+id/tanMaleImageButtonID"
        app:layout_constraintStart_toStartOf="@+id/americaTheeGuideTextViewID2"
        app:layout_constraintTop_toBottomOf="@+id/americaTheeGuideTextViewID2"
        app:srcCompat="@drawable/whitemale" />

    <ImageButton
        android:id="@+id/tanMaleImageButtonID"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        app:layout_constraintEnd_toStartOf="@+id/tannerMaleImageButtonID"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/whiteMaleImageButtonID"
        app:layout_constraintTop_toBottomOf="@+id/americaTheeGuideTextViewID2"
        app:srcCompat="@drawable/tanmale" />

    <ImageButton
        android:id="@+id/tannerMaleImageButtonID"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintEnd_toStartOf="@+id/blackMaleImageButtonID"
        app:layout_constraintHorizontal_bias="0.461"
        app:layout_constraintStart_toEndOf="@+id/tanMaleImageButtonID"
        app:layout_constraintTop_toBottomOf="@+id/americaTheeGuideTextViewID2"
        app:srcCompat="@drawable/tannermale" />

    <ImageButton
        android:id="@+id/blackMaleImageButtonID"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="12dp"
        android:layout_marginRight="12dp"
        app:layout_constraintEnd_toEndOf="@+id/americaTheeGuideTextViewID2"
        app:layout_constraintTop_toBottomOf="@+id/americaTheeGuideTextViewID2"
        app:srcCompat="@drawable/blackmale" />

    <ImageButton
        android:id="@+id/whiteFemaleImageButtonID"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/whiteMaleImageButtonID"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="@+id/whiteMaleImageButtonID"
        app:layout_constraintTop_toBottomOf="@+id/whiteMaleImageButtonID"
        app:srcCompat="@drawable/whitefemale" />

    <ImageButton
        android:id="@+id/tanFemaleImageButtonID"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/tanMaleImageButtonID"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="@+id/tanMaleImageButtonID"
        app:layout_constraintTop_toBottomOf="@+id/tanMaleImageButtonID"
        app:srcCompat="@drawable/tanfemale" />

    <ImageButton
        android:id="@+id/tannerFemaleImageButtonID"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/tannerMaleImageButtonID"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="@+id/tannerMaleImageButtonID"
        app:layout_constraintTop_toBottomOf="@+id/tannerMaleImageButtonID"
        app:srcCompat="@drawable/tannerfemale" />

    <ImageButton
        android:id="@+id/blackFemaleImageButtonID"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/blackMaleImageButtonID"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/blackMaleImageButtonID"
        app:layout_constraintTop_toBottomOf="@+id/blackMaleImageButtonID"
        app:srcCompat="@drawable/blackfemale" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="33dp"
        android:layout_height="46dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintBottom_toTopOf="@+id/imageView2"
        app:layout_constraintEnd_toStartOf="@+id/whiteMaleImageButtonID"
        app:layout_constraintHorizontal_bias="0.567"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/whiteMaleImageButtonID"
        app:layout_constraintVertical_bias="0.862"
        app:srcCompat="@drawable/greendollarsymbol" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="35dp"
        android:layout_height="49dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginBottom="32dp"
        app:layout_constraintBottom_toTopOf="@+id/whiteFemaleImageButtonID"
        app:layout_constraintEnd_toStartOf="@+id/whiteMaleImageButtonID"
        app:srcCompat="@drawable/greendollarsymbol" />
</androidx.constraintlayout.widget.ConstraintLayout>